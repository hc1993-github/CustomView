<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/activitiy_home_blueteeth_dialog_adapter_item_relay"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <TextView
        android:id="@+id/activitiy_home_blueteeth_dialog_adapter_item_tv_name"
        android:textColor="#000000"
        android:textSize="16sp"
        android:gravity="left"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/activitiy_home_blueteeth_dialog_adapter_item_tv_address"
        android:textColor="#000000"
        android:textSize="16sp"
        android:gravity="left"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/activitiy_home_blueteeth_dialog_adapter_item_tv_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <View
        android:id="@+id/bottm_view"
        android:layout_below="@id/activitiy_home_blueteeth_dialog_adapter_item_tv_address"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#cccccc"
        />
</RelativeLayout>